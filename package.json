{
  "name": "symfony-components",
  "version": "1.0.0",
  "scripts": {
    "build": "rollup -c",
    "dev": "rollup -c -w",
    "start": "sirv public",
    "validate": "svelte-check",
    "test": "jest"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.12.16",
    "@rollup/plugin-commonjs": "^14.0.0",
    "@rollup/plugin-node-resolve": "^8.0.0",
    "@rollup/plugin-typescript": "^6.1.0",
    "@sveltestack/svelte-query": "^1.0.8",
    "@testing-library/jest-dom": "^5.11.9",
    "@testing-library/svelte": "^3.0.3",
    "@tsconfig/svelte": "^1.0.10",
    "@types/jest": "^26.0.20",
    "babel-jest": "^26.6.3",
    "fusioncharts": "^3.16.0",
    "jest": "^26.6.3",
    "prettier": "^2.2.1",
    "prettier-plugin-svelte": "^2.1.4",
    "rollup": "^2.39.0",
    "rollup-plugin-livereload": "^2.0.0",
    "rollup-plugin-svelte": "^6.1.1",
    "rollup-plugin-terser": "^7.0.0",
    "svelte": "^3.32.3",
    "svelte-check": "^1.1.34",
    "svelte-fusioncharts": "^1.0.0",
    "svelte-jester": "^1.3.0",
    "svelte-preprocess": "^4.6.8",
    "svelte-progress-bar": "^2.0.0",
    "ts-jest": "^26.5.1",
    "tslib": "^2.1.0",
    "typescript": "^3.9.9"
  },
  "dependencies": {
    "@pixss/svelte": "^1.2.2",
    "sirv-cli": "^1.0.11",
    "svelte-markdown": "0.0.4"
  },
  "jest": {
    "transform": {
      "^.+\\.svelte$": [
        "svelte-jester",
        {
          "preprocess": true
        }
      ],
      "^.+\\.js$": "babel-jest",
      "^.+\\.ts$": "ts-jest"
    },
    "moduleFileExtensions": [
      "js",
      "ts",
      "svelte"
    ],
    "setupFilesAfterEnv": [
      "@testing-library/jest-dom/extend-expect"
    ]
  }
}
